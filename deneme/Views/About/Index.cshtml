@model IEnumerable<About>

@{
    ViewData["Title"] = "Index";
}


@foreach(var obj in Model){

    <div>
        <div>
            <h2>@obj.Title</h2>
            <p>@obj.Body</p>
            <img src="@obj.Image"></img>
        </div>

        <a asp-controller="About" asp-action="Edit" asp-route-id="@obj.Id">Edit</a>

    </div>
}





